<%= form_with url: search_path, method: :get, local: true, class: 'search-container form-inline my-2 my-lg-0' do |f| %>
  <%= f.text_field :content, placeholder: 'Search', class: "form-control mr-sm-2" %>
  <%= f.select :model, options_for_select({ "Team" => "team", "Part" => "tag" }), {include_blank: "Coose"}, {class: 'form-control mr-sm-2'} %>
  <%= f.submit '検索', class: 'btn btn-outline-success my-2 my-sm-0' %>
<% end %>
